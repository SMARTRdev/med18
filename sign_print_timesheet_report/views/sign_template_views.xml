<odoo>
    <record id="view_sign_print_timesheet_report_template_form" model="ir.ui.view">
        <field name="name">view.sign.print.timesheet.report.template.form</field>
        <field name="model">sign.template</field>
        <field name="inherit_id" ref="sign.sign_template_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='authorized_ids']" position="after">
                <field name="use_as_sign_print_timesheet_report"/>
            </xpath>
        </field>
    </record>
</odoo>